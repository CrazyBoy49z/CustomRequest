{
  "name": "CustomRequest",
  "lowCaseName": "customrequest",
  "description": "Use friendly URLs everywhere",
  "author": "Thomas Jakobi",
  "version": "1.2.6",
  "package": {
    "menus": [
      {
        "text": "customrequest.menu_home",
        "description": "customrequest.menu_home_desc",
        "action": "home",
        "parent": "components",
        "namespace": "customrequest"
      }
    ],
    "elements": {
      "plugins": [
        {
          "name": "CustomRequest",
          "file": "customrequest.plugin.php",
          "events": [
            "OnPageNotFound",
            "OnDocFormSave",
            "OnDocFormDelete",
            "OnDocPublished",
            "OnDocUnPublished",
            "OnSiteRefresh"
          ]
        }
      ]
    },
    "systemSettings": [
      {
        "key": "debug",
        "type": "combo-boolean",
        "area": "system",
        "value": ""
      }
    ]
  },
  "database": {
    "tables": [
      "CustomrequestConfigs"
    ]
  },
  "build": {
    "readme": "docs/readme.txt",
    "license": "docs/license.txt",
    "changelog": "docs/changelog.txt"
  }
}